
<%= form_for @post, html: {multipart: true} do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>

  <%= f.label :user_id %>
  <%= f.text_field :user_id %>
  <br>


  <%= f.label :content %>
  <%= f.text_field :content %>
  <br>

  <%= f.label :image %>
  <%#= f.text_field :image %>
  <%= f.cl_image_upload :image %>
  <br>

  <%= f.submit %>

<% end %>
